msgid "フレーム"
msgstr "Frame"

#: ../../source/man2/operation_vrm2.rst:1
msgid "VRM"
msgstr "VRM"

#: ../../source/man2/operation_vrm2.rst:1
msgid "その他オブジェクト"
msgstr "Other objects"

#: ../../source/man2/operation_vrm2.rst:1
msgid "1"
msgstr "1"

#: ../../source/man2/operation_vrm2.rst:1
msgid "右手を所定の位置に動かす"
msgstr "move the right hand to the desired position"

#: ../../source/man2/operation_vrm2.rst:1
msgid "剣を所定の位置に動かす"
msgstr "Move the sword to the specified position"

